<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg" style= "z-index: 1060">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close fui-cross" data-dismiss="modal" aria-hidden="true"></button>

        <h4 class="modal-title" id="myModalLabel">Upload New File</h4>
      </div>

     <%= form_for(@team, url: {action: "update"}, html: { method: :put }) do |f| %>
      <div class="modal-body">
        <div><%= f.label "Title" %><br />
        <%if !@team.files_file_name.blank?%>
          <%if !@team.files1_file_name.blank?%>
            <%if !@team.files2_file_name.blank?%>
              <%if !@team.files3_file_name.blank?%>
                <%if !@team.files4_file_name.blank?%>
                  <%if !@team.files5_file_name.blank?%>
                    <%if !@team.files6_file_name.blank?%>
                      <%if !@team.files7_file_name.blank?%>
                        <%if !@team.files8_file_name.blank?%>
                          <%if !@team.files9_file_name.blank?%>
                           <%=p Error%>
                          <%else%>
                            <%= f.text_field :file9Title %></div>
                          <%end%>
                        <%else%>
                          <%= f.text_field :file8Title %></div>
                        <%end%>
                      <%else%>
                       <%= f.text_field :file7Title %></div>
                      <%end%>
                    <%else%>
                      <%= f.text_field :file6Title %></div>
                    <%end%>
                  <%else%>
                    <%= f.text_field :file5Title %></div>
                  <%end%>
                <%else%>
                  <%= f.text_field :file4Title %></div>
                <%end%>  
              <%else%>
                <%= f.text_field :file3Title %></div>
              <%end%>
            <%else%>
              <%= f.text_field :file2Title %></div>
            <%end%>
          <%else%>
            <%= f.text_field :file1Title %></div>
          <%end%>
        <%else%>
          <%= f.text_field :fileTitle %></div>
        <%end%>

      <div><%= f.label "Description" %><br />
      <%if !@team.files_file_name.blank?%>
          <%if !@team.files1_file_name.blank?%>
            <%if !@team.files2_file_name.blank?%>
              <%if !@team.files3_file_name.blank?%>
                <%if !@team.files4_file_name.blank?%>
                  <%if !@team.files5_file_name.blank?%>
                    <%if !@team.files6_file_name.blank?%>
                      <%if !@team.files7_file_name.blank?%>
                        <%if !@team.files8_file_name.blank?%>
                          <%if !@team.files9_file_name.blank?%>
                           <%=p Error%>
                          <%else%>
                            <%= f.text_area :file9_description %></div>
                          <%end%>
                        <%else%>
                          <%= f.text_area :file8_description %></div>
                        <%end%>
                      <%else%>
                       <%= f.text_area :file7_description %></div>
                      <%end%>
                    <%else%>
                      <%= f.text_area :file6_description %></div>
                    <%end%>
                  <%else%>
                    <%= f.text_area :file5_description %></div>
                  <%end%>
                <%else%>
                  <%= f.text_area :file4_description %></div>
                <%end%>  
              <%else%>
                <%= f.text_area :file3_description %></div>
              <%end%>
            <%else%>
              <%= f.text_area :file2_description %></div>
            <%end%>
          <%else%>
            <%= f.text_area :file1_description %></div>
          <%end%>
        <%else%>
          <%= f.text_area :file_description %></div>
        <%end%>
       

    <div><%= f.label "File"%> <i>(Please convert all possible files to PDF)</i>
<br/>
      <div class="form-group">
  <div class="fileinput fileinput-new" data-provides="fileinput">
    <div class="input-group">
      <div class="form-control uneditable-input" data-trigger="fileinput">
        <span class="fui-clip fileinput-exists"></span>
        <span class="fileinput-filename"></span>
      </div>
